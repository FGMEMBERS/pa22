<?xml version="1.0"?>

<PropertyList>

  <path>oat.ac</path>

  <animation>
    <type>material</type>
    <object-name>case</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>0.03</red>
      <green>0.03</green>
      <blue>0.02</blue>
      <factor-prop>systems/electrical/outputs/cabin-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>environment/temperature-degf</property>
    <factor>1.39</factor>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

<!-- *** LoD *** -->
  <animation> 
    <type>range</type>
    <min-m>0</min-m>
    <max-m>50</max-m>
  </animation>

</PropertyList>
