<?xml version="1.0"?>

<PropertyList>

  <path>oil.ac</path>

  <animation>
    <type>material</type>
    <object-name>Circle</object-name>
    <object-name>temp_needle</object-name>
    <object-name>pres_needle</object-name>
    <emission>
      <red>1.0</red>
      <green>0.6</green>
      <blue>0.3</blue>
      <factor-prop>sim/model//material/instruments/factor</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pres_needle</object-name>
    <property>engines/engine/oil-pressure-psi</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>20</ind>
        <dep>32.0</dep>
      </entry>
      <entry>
        <ind>40</ind>
        <dep>69.0</dep>
      </entry>
      <entry>
        <ind>60</ind>
        <dep>111.0</dep>
      </entry>
      <entry>
        <ind>80</ind>
        <dep>148.0</dep>
      </entry>
      <entry>
        <ind>100</ind>
        <dep>180.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>0.005</z-m>
    </center>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>temp_needle</object-name>
    <property>engines/engine/oil-temperature-degf</property>
    <interpolation>
      <entry>
        <ind>60</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>100</ind>
        <dep>11.5</dep>
      </entry>
      <entry>
        <ind>140</ind>
        <dep>29.7</dep>
      </entry>
      <entry>
        <ind>180</ind>
        <dep>57.8</dep>
      </entry>
      <entry>
        <ind>200</ind>
        <dep>77.0</dep>
      </entry>
      <entry>
        <ind>220</ind>
        <dep>99.5</dep>
      </entry>
      <entry>
        <ind>240</ind>
        <dep>125.0</dep>
      </entry>
      <entry>
        <ind>260</ind>
        <dep>152.0</dep>
      </entry>
      <entry>
        <ind>280</ind>
        <dep>180.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.004</z-m>
    </center>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

<!-- *** LoD *** -->
  <animation> 
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

</PropertyList>
