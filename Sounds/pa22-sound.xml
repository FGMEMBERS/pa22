  <switch>
	<name>on</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <property>sim/sound/switch</property>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.3</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </switch>

  <switch>
	<name>off</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <not>
        <property>sim/sound/switch</property>
      </not>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.3</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </switch>

  <nav-switch>
	<name>nav on</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <property>/controls/switches/nav-lights</property>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.5</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </nav-switch>

  <nav-switch>
	<name>nav off</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <not>
        <property>/controls/switches/nav-lights</property>
      </not>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.5</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </nav-switch>

  <squish>
	<name>primer out</name>
	<mode>once</mode>
	<path>Sounds/squeal.wav</path>
    <condition>
      <property>sim/model/primer-pos</property>
    </condition>
	<position>
		<x>-0.2</x>
		<y>0.1</y> 
		<z>0.3</z>
	</position>
	<volume>
		<factor>0.05</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </squish>

  <squish>
	<name>primer in</name>
	<mode>once</mode>
	<path>Sounds/squeal.wav</path>
    <condition>
      <not>
        <property>sim/model/primer-pos</property>
      </not>
    </condition>
	<position>
		<x>-0.2</x>
		<y>0.1</y> 
		<z>0.3</z>
	</position>
	<volume>
		<factor>0.05</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </squish>

  <lights-switch>
	<name>landing</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <equals>
        <property>/controls/switches/landing-taxi-lights</property>
        <value>0</value>
      </equals>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.3</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </lights-switch>

  <lights-switch>
	<name>lights off</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <equals>
        <property>/controls/switches/landing-taxi-lights</property>
        <value>1</value>
      </equals>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.3</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </lights-switch>

  <lights-switch>
	<name>taxi</name>
	<mode>once</mode>
	<path>Sounds/click.wav</path>
    <condition>
      <equals>
        <property>/controls/switches/landing-taxi-lights</property>
        <value>2</value>
      </equals>
    </condition>
	<position>
		<x>-0.3</x>
		<y>0.2</y> 
		<z>0.6</z>
	</position>
	<volume>
		<factor>0.3</factor>
	</volume>
	<reference-dist>15</reference-dist>
	<max-dist>5</max-dist>
  </lights-switch>
  
