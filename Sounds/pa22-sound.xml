  <rain>
    <name>rain</name>
    <mode>looped</mode>
    <path>Aircraft/pa22/Sounds/rain_canopy.wav</path>
    <condition>
      <greater-than>
        <property>/sim/model/rain/raining-norm</property>
        <value>0.2</value>
      </greater-than>
    </condition>
    <volume>
      <property>/sim/model/rain/raining-norm</property>
      <factor>1.00</factor>
      <offset>0.00</offset>
      <min>0.4</min>
      <max>0.9</max>
    </volume>
    <pitch>
      <property>/velocities/airspeed-kt</property>
      <factor>0.001</factor>
      <offset>1.0</offset>
    </pitch>
    <volume>
	  <property>/sim/model/sound/wind_volume</property>
	  <min>0.5</min>
	  <max>10.0</max>
	  <max-dist>5.0</max-dist>
    </volume>
  </rain>

</fx>
 
</PropertyList>
